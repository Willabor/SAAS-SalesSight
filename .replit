modules = ["nodejs-20", "web", "postgresql-16", "python-3.11"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist", "ml_service/venv", "ml_service/__pycache__"]

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33835
externalPort = 3002

[[ports]]
localPort = 36091
externalPort = 3003

[[ports]]
localPort = 36117
externalPort = 3000

[[ports]]
localPort = 37061
externalPort = 4200

[[ports]]
localPort = 39603
externalPort = 3001

[env]
PORT = "5000"
ML_SERVICE_URL = "http://localhost:8000"

[agent]
integrations = ["javascript_database:1.0.0", "javascript_object_storage:1.0.0", "javascript_log_in_with_replit:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000
